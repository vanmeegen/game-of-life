{
  "name": "game-of-life",
  "version": "0.5.0",
  "author": "Marco van Meegen",
  "description": "React/MobX Game of Life",
  "scripts": {
    "start": "npm run devserver",
    "dist": "set NODE_ENV=production & webpack -p --progress --config webpack.config_prod.js",
    "dist-dev": "webpack -p --progress --config webpack.config.js",
    "dist-perftest": "webpack -p --progress --config webpack.config_perftest.js",
    "devserver": "webpack-dev-server --progress --colors --hot --content-base public --host 0.0.0.0",
    "gridserver": "webpack-dev-server --progress --colors --hot --content-base public --config webpack.config_grid.js",
    "prodserver": "npm run dist && http-server public --cors --proxy http://localhost:9091",
    "tslint": "tslint -r node_modules/tslint-microsoft-contrib -c tslint.json src/**/*.ts{,x} test/**/*.ts{,x}",
    "tslintfix": "tslint -r node_modules/tslint-microsoft-contrib -c tslint.json src/**/*.ts{,x} test/**/*.ts{,x} --fix",
    "test": "mocha test test/**/*.spec.ts test/**/*.spec.tsx",
    "test:watch": "npm run test -- --watch",
    "test:karma": "set REPORT_FILE=./results/karmatests.xml & karma start --single-run",
    "test:karma:watch": "set REPORT_FILE=./results/karmatests.xml & karma start",
    "version": "node ./echoversion.js"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "es6-geometry": "1.3.0",
    "es6-shim": "0.35.3",
    "immutable": "3.8.1",
    "loglevel": "1.4.1",
    "mobx": "^3.1.2",
    "mobx-react": "^4.1.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-loading-bar": "0.0.3",
    "shallowequal": "0.2.2",
    "stimo": "0.6.0"
  },
  "devDependencies": {
    "@types/chai": "3.4.30",
    "@types/enzyme": "2.7.4",
    "@types/immutable": "3.8.1",
    "@types/loglevel": "1.4.28",
    "@types/mocha": "2.2.29",
    "@types/node": "6.0.38",
    "@types/react": "15.0.0",
    "@types/react-addons-perf": "0.14.18",
    "@types/react-dom": "0.14.20",
    "@types/shallowequal": "0.2.1",
    "@types/vkbeautify": "0.99.0",
    "@types/webpack": "^2.2.7",
    "@types/whatwg-fetch": "0.0.28",
    "chai": "4.0.0-canary.1",
    "css-loader": "0.23.1",
    "enzyme": "2.7.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.3",
    "isomorphic-fetch": "2.2.1",
    "jsdom": "9.3.0",
    "jsdom-global": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-edge-launcher": "0.2.0",
    "karma-es6-shim": "1.0.0",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jenkins-reporter": "0.0.2",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mocha": "2.5.3",
    "phantomjs": "2.1.7",
    "react-addons-perf": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "2.0.0",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "tslint-loader": "3.4.2",
    "tslint-microsoft-contrib": "2.0.8",
    "typescript": "2.2.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "whatwg-fetch": "1.0.0"
  }
}
